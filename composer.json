{
  "name": "wenbinye/tars",
  "description": "tars php server",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ye Wenbin",
      "email": "wenbinye@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "wenbinye\\tars\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "wenbinye\\tars\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "doctrine/annotations": "^1.8",
    "php-di/phpdoc-reader": "^2.1",
    "psr/container": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/simple-cache": "^1.0",
    "kuiper/kuiper": "0.5.0.x-dev",
    "symfony/console": "^5.0",
    "symfony/validator": "^5.0",
    "webmozart/assert": "^1.6",
    "laminas/laminas-code": "^3.4",
    "laminas/laminas-diactoros": "^2.2"
  },
  "require-dev": {
    "ext-sockets": "*",
    "copyrenzhe/tars-ide-helper": "dev-master",
    "mockery/mockery": "^1.3",
    "monolog/monolog": "^2.0",
    "php-di/php-di": "^6.0",
    "phpunit/phpunit": "^8.4",
    "slim/slim": "^4.3",
    "swoft/swoole-ide-helper": "^4.4",
    "symfony/event-dispatcher": "^5.0",
    "symfony/finder": "^5.0",
    "wenbinye/tars-gen": "dev-master",
    "winwin/composer-phpcs-plugin": "^0.1"
  },
  "repositories": {
    "kuiper": {
      "type": "path",
      "url": "/home/ywb/work/kuiper/kuiper"
    },
    "tars-gen": {
      "type": "path",
      "url": "/home/ywb/src/php/tars-gen"
    }
  },
  "scripts": {
    "gen": "./vendor/bin/tars-gen"
  },
  "extra": {
    "tars": {
      "generator": {
        "client": [
          {
            "namespace": "wenbinye\\tars\\stat",
            "tars_path": "tars/StatF.tars",
            "output": "src/stat",
            "servants": {
              "StatF": "tars.tarsstat.StatObj"
            }
          },
          {
            "namespace": "wenbinye\\tars\\stat",
            "tars_path": "tars/PropertyF.tars",
            "output": "src/stat",
            "servants": {
              "PropertF": "tars.tarsproperty.PropertyObj"
            }
          },
          {
            "namespace": "wenbinye\\tars\\stat",
            "tars_path": "tars/NodeF.tars",
            "output": "src/stat",
            "servants": {
              "NodeF": "tars.tarsnode.ServerObj"
            }
          },
          {
            "namespace": "wenbinye\\tars\\log",
            "tars_path": "tars/LogF.tars",
            "output": "src/log",
            "servants": {
              "LogF": "tars.tarslog.LogObj"
            }
          },
          {
            "namespace": "wenbinye\\tars\\registry",
            "tars_path": "tars/QueryF.tars",
            "output": "src/registry",
            "servants": {
              "QueryF": "tars.tarsregistry.QueryObj"
            }
          },
          {
            "namespace": "wenbinye\\tars\\config",
            "tars_path": "tars/ConfigF.tars",
            "output": "src/config",
            "servants": {
              "ConfigF": "tars.tarsconfig.ConfigObj"
            }
          }
        ]
      }
    }
  }
}
