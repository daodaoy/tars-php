{
  "name": "wenbinye/tars",
  "description": "tars php server",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ye Wenbin",
      "email": "wenbinye@gmail.com"
    }
  ],
  "minimum-stability": "dev",
